{
  "include": [
    "./types/**/*.d.ts"
  ],
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "target": "ES6",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "declaration": false,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "esModuleInterop": true
  }
}
